{
  "name": "launchpad",
  "compatibility_date": "2025-12-06",
  "assets": {
    "directory": ".",
    "binding": "ASSETS",
    "exclude": [
      ".git/**",
      ".gitignore",
      "wrangler.jsonc",
      "schema.sql",
      "migrations/**"
    ]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "launchpad-db",
      "database_id": "YOUR_DATABASE_ID_HERE"
    }
  ]
}
